---
import Layout from "@layouts/Layout.astro";
import WpFlexibleContent from "@components/wordpress/WpFlexibleContent.astro";

const { wordpress } = Astro.props;
const page = wordpress.page;
---

<Layout wordpress={wordpress} title={page.title.rendered}>
  <WpFlexibleContent items={page.acf.flexible_content} />
</Layout>
